# Advanced Python Backend Generator Configuration

# Core Settings
OLLAMA_HOST=http://localhost:11434
OLLAMA_MODEL=deepseek-r1:latest
OUTPUT_DIRECTORY=./generated_projects

# AI Model Settings
TEMPERATURE=0.1
MAX_TOKENS=8192
TOP_P=0.9
TOP_K=40
REPEAT_PENALTY=1.1

# Generation Settings
DEFAULT_FRAMEWORK=fastapi
DEFAULT_DATABASE=sqlite
ENABLE_TESTING=true
ENABLE_DOCKER=true
ENABLE_LINTING=true

# Development Settings
DEBUG=true
LOG_LEVEL=INFO
ENABLE_TELEMETRY=false

# Security Settings
SECRET_KEY=your-secret-key-change-in-production
CORS_ORIGINS=*
RATE_LIMIT_ENABLED=false

# Logging
LOG_FILE=logs/generator.log
LOG_ROTATION=daily
LOG_RETENTION_DAYS=30

# Optional: GitHub Integration
GITHUB_TOKEN=your_github_token_here
GITHUB_USERNAME=your_username

# Optional: Docker Settings
DOCKER_ENABLED=true
DOCKER_REGISTRY=your-registry.com
DOCKER_NAMESPACE=your-namespacet-File -FilePath ".env.example" -Encoding UTF8